<?xml version="0.0" ?>
<robot xmlns:xacro="http://ros.org/wiki/xacro" name="model.urdf">

  <link name="baseLink">
    <contact>
      <friction_anchor/>
      <lateral_friction value="1"/>
      <spinning_friction value="0.001"/>
      <rolling_friction value="0.001"/>
      <contact_cfm value="0.5"/>
      <contact_erp value="0.5"/>
    </contact>
    <inertial>
       <origin rpy="0 0 0.0" xyz="-0.005 0.013 0.0"/>
       <mass value=".404"/>
       <inertia ixx="1e-3" ixy="0" ixz="0" iyy="1e-3" iyz="0" izz="1e-3"/>
    </inertial>
    <visual>
      <geometry>
        <mesh filename="package://objects_models/urdf/002_master_chef_can/textured_simple_reoriented.obj" scale="0.7 0.7 0.7"/>
      </geometry>
      <material name="white">
        <color rgba="1. 1. 1. 1."/>
      </material>
    </visual>
    <collision>
      <geometry>
    	 	<mesh filename="package://objects_models/urdf/002_master_chef_can/collision_vhacd.obj" scale="0.7 0.7 0.7"/>
      </geometry>
    </collision>
  </link>

  <link name="dummy">
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <mass value="0.01"/>
        <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0" izz="0.0" />
    </inertial>
  </link>
  <joint name="dummy_joint" type="fixed">
    <parent link="dummy"/>
    <child link="baseLink"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>
</robot>
